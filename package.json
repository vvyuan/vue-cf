{
  "name": "vue-cf",
  "version": "0.0.29",
  "description": "基于antd和vue的中后台前端快速开发组件",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "vue-cf": "tools/help.js",
    "cfc": "tools/CFConfigCreator.js",
    "cfConfigCreator": "tools/CFConfigCreator.js"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node tools/help.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitee.com:VYuan/vue-cf.git"
  },
  "keywords": [
    "antd",
    "vue",
    "component",
    "快速开发"
  ],
  "author": "VYuan",
  "private": false,
  "license": "Apache-2.0",
  "dependencies": {
    "colors": "^1.4.0",
    "inquirer": "^7.0.6",
    "mysql": "^2.18.1"
  },
  "peerDependencies": {
    "ant-design-vue": "^1.4.12",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "uuid": "^7.0.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-sucrase": "^3.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@types/md5": "^2.1.33",
    "@types/uuid": "^7.0.0",
    "ant-design-vue": "^1.4.12",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "uuid": "^7.0.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "core-js": "^2.6.11",
    "less": "^3.11.1",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-vue2": "^0.8.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "rules": {
      "quotes": "off",
      "object-curly-spacing": "off",
      "semi": "off",
      "comma-spacing": "off",
      "comma-dangle": [
        "error",
        {
          "arrays": "ignore",
          "objects": "ignore",
          "imports": "ignore",
          "exports": "ignore",
          "functions": "never"
        }
      ],
      "no-sequences": "off",
      "vue/no-unused-vars": "warn",
      "eqeqeq": "warn",
      "no-trailing-spaces": "off",
      "space-before-blocks": "off",
      "space-infix-ops": "warn",
      "space-unary-ops": "warn",
      "space-in-parens": "warn",
      "space-before-function-paren": "off",
      "keyword-spacing": "off",
      "arrow-spacing": "off",
      "indent": "off"
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
